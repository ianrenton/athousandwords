RewriteEngine On
RewriteRule ^about$ about.php [L]
RewriteRule ^login$ login.php [L]
RewriteRule ^logout$ logout.php [L]
RewriteRule ^editprofile$ editprofile.php [L]
RewriteRule ^register$ register.php [L]
RewriteRule ^submitpicture$ submitpicture.php [L]
RewriteRule ^user\/([\d]+)$ user.php?id=$1 [L]
RewriteRule ^submitstory\/([\d]+)$ submitstory.php?pic=$1 [L]
RewriteRule ^editstory\/([\d]+)$ editstory.php?id=$1 [L]
RewriteRule ^story\/([\d]+)$ story.php?id=$1 [L]
RewriteRule ^([\d]+)\/([\w]+)$ index.php?pic=$1&sort=$2 [L]
